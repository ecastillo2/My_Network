<!doctype html>
<html lang="en">

<head>
    <title>Elements | Klorofil - Free Bootstrap Dashboard Template</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <!-- VENDOR CSS -->
    <link rel="stylesheet" href="assets/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="assets/vendor/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="assets/vendor/linearicons/style.css">
    <!-- MAIN CSS -->
    <link rel="stylesheet" href="assets/css/main.css">
    <!-- FOR DEMO PURPOSES ONLY. You should remove this in your project -->
    <link rel="stylesheet" href="assets/css/demo.css">
    <!-- GOOGLE FONTS -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,600,700" rel="stylesheet">
    <!-- ICONS -->
    <link rel="apple-touch-icon" sizes="76x76" href="assets/img/apple-icon.png">
    <link rel="icon" type="image/png" sizes="96x96" href="assets/img/favicon.png">
</head>

<body>
    <!-- WRAPPER -->
    <div id="wrapper">
        <!-- NAVBAR -->
      
      
        <!-- MAIN -->
        <div class="main">
            <!-- MAIN CONTENT -->
            <div class="main-content">
                <div class="container-fluid">
                    <h3 class="page-title">Add Contact</h3>
                    <div class="row">
                        <div class="col-md-6">

                            <!-- BUTTONS -->
                            <div class="panel">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Profile</h3>
                                </div>
                                <div class="panel-body">
                                    <input type="text" class="form-control" name="category" placeholder="Category">
                                    <br>
                                    <input type="text" class="form-control" name="firstName" placeholder="First Name">
                                    <br>
                                    <input type="text" class="form-control" name="lastName" placeholder="Last Name">
                                    <br>
                                    <input type="text" class="form-control" name="age" placeholder="Age">
                                    <br>
                                    <input type="text" class="form-control" name="birthday" placeholder="Birthday">
                                    <br>
                                    <input type="text" class="form-control" name="primaryPhoneNumber" placeholder="Primary Phone Number">
                                    <br>
                                    <input type="text" class="form-control" name="secondaryPhoneNumber" placeholder="Secondary Phone Number">
                                    <br>
                                    <input type="text" class="form-control" name="image" placeholder="Image">
                                    <br>
                                    <input type="text" class="form-control" name="relationship" placeholder="Relationship">
                                    <br>
                                    <input type="text" class="form-control" name="hobbies" placeholder="Hobbies">
                                </div>
                            </div>
                            <!-- END BUTTONS -->
                            <!-- INPUTS -->
                            <div class="panel">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Social</h3>
                                </div>
                                <div class="panel-body">
                                    <input type="text" class="form-control" name="linkedInURL" placeholder="LinkedInURL">
                                    <br>
                                    <input type="text" class="form-control" name="faceBookURL" placeholder="FaceBookURL">
                                    <br>
                                    <input type="text" class="form-control" name="instagramURL" placeholder="InstagramURL">
                                    <br>
                                    <input type="text" class="form-control" name="twitterURL" placeholder="TwitterURL">
                                    <br>
                                    <input type="text" class="form-control" name="socialGroups" placeholder="Social Groups">
                                    
                                </div>
                            </div>
                            <!-- END INPUTS -->
                            <!-- INPUT SIZING -->
                            <div class="panel">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Family</h3>
                                </div>
                                <div class="panel-body">
                                    <input type="text" class="form-control" name="spouseFirstName" placeholder="Spouse First Name">
                                    <br>
                                    <input type="text" class="form-control" name="spouseLastName" placeholder="Spouse Last Name">
                                    <br>
                                    <input type="text" class="form-control" name="hasChildren" placeholder="Has Children">
                                    <br>
                                    <input type="text" class="form-control" name="fatherFirstName" placeholder="Father First Name">
                                    <br>
                                    <input type="text" class="form-control" name="fatherLastName" placeholder="Father Last Name">
                                    <br>
                                    <input type="text" class="form-control" name="motherFirstName" placeholder="Mother First Name">
                                    <br>
                                    <input type="text" class="form-control" name="motherLastName" placeholder="Mother Last Name">
                                    <br>
                                    <input type="text" class="form-control" name="hasPets" placeholder="Has Pets">

                                </div>
                            </div>
                            <!-- END INPUT SIZING -->
                        </div>
                        <div class="col-md-6">
                            <!-- LABELS -->
                            <div class="panel">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Occupation</h3>
                                </div>
                                <div class="panel-body">
                                    <input type="text" class="form-control" name="primaryOccupation" placeholder="Primary Occupation">
                                    <br>
                                    <input type="text" class="form-control" name="secondaryOccupation" placeholder="Secondary Occupation">
                                    <br>
                                    <input type="text" class="form-control" name="companyName" placeholder="Company Name">
                                    <br>
                                    <input type="text" class="form-control" name="companyAddress" placeholder="Company Address">
                                    <br>
                                    <input type="text" class="form-control" name="salaryRange" placeholder="SalaryRange">
                                    <br>
                                    <input type="text" class="form-control" name="skill" placeholder="Skill">
                                    <br>
                                    <input type="text" class="form-control" name="businessCardFrontImg" placeholder="Business Card Front Img">
                                    <br>
                                    <input type="text" class="form-control" name="businessCardBackImg" placeholder="Business Card Back Img">

                                </div>
                            </div>
                            <!-- END LABELS -->
                            <!-- PROGRESS BARS -->
                            <div class="panel">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Peronal</h3>
                                </div>
                                <div class="panel-body">
                                    <input type="text" class="form-control" name="metLocation" placeholder="Met Location">
                                    <br>
                                    <input type="text" class="form-control" name="numberOfYearKnown" placeholder="Number Of Year Known">
                                    <br>
                                    <input type="text" class="form-control" name="lastSpokeToDate" placeholder="Last Spoke To Date">
                                    <br>
                                    <input type="text" class="form-control" name="personality" placeholder="Personality">
                                    <br>
                                    <input type="text" class="form-control" name="relationshipStatus" placeholder="Relationship Status">

                                </div>
                            </div>
                            <!-- END PROGRESS BARS -->
                            <!-- INPUT GROUPS -->
                            <div class="panel">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Education</h3>
                                </div>
                                <div class="panel-body">
                                    <input type="text" class="form-control" name="inSchool" placeholder="In School">
                                    <br>
                                    <input type="text" class="form-control" name="collegeAttened" placeholder="College Attended">
                                    <br>
                                    <input type="text" class="form-control" name="highSchoolAttened" placeholder="HighSchool Attended">
                                    <br>
                                    <input type="text" class="form-control" name="degree" placeholder="Degree">
                                    <br>
                                    <input type="text" class="form-control" name="personality" placeholder="Personality">

                                </div>
                            </div>
                            <!-- END INPUT GROUPS -->
                           
                        </div>

                    </div>
                </div>
            </div>
            <!-- END MAIN CONTENT -->
        </div>
        <!-- END MAIN -->
        <div class="clearfix"></div>
        <footer>
            <div class="container-fluid">
                <p class="copyright">&copy; 2019 <a href="https://www.userxcel.com" target="_blank">User Xcel</a></p>
            </div>
        </footer>
    </div>
    <!-- END WRAPPER -->
    <!-- Javascript -->
    <script src="assets/vendor/jquery/jquery.min.js"></script>
    <script src="assets/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="assets/vendor/jquery-slimscroll/jquery.slimscroll.min.js"></script>
    <script src="assets/scripts/klorofil-common.js"></script>
</body>

</html>
